{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "types": ["vite/client", "@nuxt/types", "@nuxt/nitro", "@types/node"],
    "baseUrl": "./",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"]
    },
    "outDir": ".output",
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node"
  }
}
